interface interrupt {
    void handler();
}

interface interrupt_i8 {
    void handler(uint8 i);
}

type void_interrupt implements interrupt {
    void handler() {}
}

type void_interrupt_i8 implements interrupt_i8 {
    void handler(uint8 i) {}
}

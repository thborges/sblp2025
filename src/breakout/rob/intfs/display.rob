enum displayorientation {NORMAL, NORMAL_MIRRORED, UPSIDE_DOWN, UPSIDE_DOWN_MIRRORED}

interface display {
    uint16 rows();
    uint16 columns();
    void init_display();
    void set_orientation(displayorientation o);
    void set_contrast(uint8 c);
    void update_frame();
    void clear();
    void set_address(uint8 a);
}
